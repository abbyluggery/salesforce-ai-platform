<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Expected_Duration_Positive</fullName>
    <active>true</active>
    <description>Ensures expected duration is positive</description>
    <errorConditionFormula>AND(
    NOT(ISBLANK(Expected_Duration_Minutes__c)),
    Expected_Duration_Minutes__c &lt;= 0
)</errorConditionFormula>
    <errorDisplayField>Expected_Duration_Minutes__c</errorDisplayField>
    <errorMessage>Expected Duration must be a positive number.</errorMessage>
</ValidationRule>
