<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <deploymentStatus>Deployed</deploymentStatus>
    <description>Tracks evening routine completion, wind-down activities, and sleep preparation to improve sleep quality</description>
    <label>Evening Routine Tracking</label>
    <pluralLabel>Evening Routine Trackings</pluralLabel>
    <nameField>
        <displayFormat>ERT-{0000}</displayFormat>
        <label>Evening Routine Number</label>
        <type>AutoNumber</type>
    </nameField>
    <sharingModel>ReadWrite</sharingModel>
    <enableReports>true</enableReports>
    <enableActivities>false</enableActivities>
    <enableBulkApi>true</enableBulkApi>
    <enableHistory>true</enableHistory>
    <enableSearch>true</enableSearch>
    <enableSharing>true</enableSharing>
    <enableStreamingApi>true</enableStreamingApi>
</CustomObject>
